{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"]
    },
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "jsx": "react",
    "lib": ["dom", "es7", "es2018", "scripthost"]
  },
  "exclude": ["node_modules"],
  "include": ["./src/*"],
  "files": ["./src/index.jsx"]
}
